@using QLNHATHAU.Models
@model ConsumptionValidation

@using (Html.BeginForm())

{
    var aa = Model;

    <div class="container">
        <div class="row">
            <h5 class="text-center" style="margin-top: 8px">Từ ngày</h5>
            <div class="col-md-3 pull-left">
                @Html.TextBox("begind", null, new { @id = "begind", @type = "date", @class = "form-control" })
            </div>
            <h5 class="text-center" style="margin-top: 8px">Đến ngày</h5>
            <div class="col-md-3">
                @Html.TextBox("endd", null, new { @id = "endd", @type = "date", @class = "form-control" })
            </div>
            <div class="col-md-4">
                <label for="date_end" style="visibility:hidden">End Date</label>
                <input type="submit" value="Tìm kiếm" class="btn btn-primary pull-left" id="btnsearch" />
            </div>
        </div>
        <div class="row">
            <h1></h1>
        </div>
    </div>
}


