@{ Layout = null;}
@model QLNHATHAU.Models.DangKyHocAnToanValidation
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h5 id="ModalTitle"></h5>
            <a href="#" class="close" data-dismiss="modal">&times;</a>
        </div>
        <div class="modal-body">
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <fieldset id="SubmitForm">
                <h3> <i class="fa fa-table"></i> Thông tin chung</h3>
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover" cellspacing="0">
                        <tr>
                            <td>Số hợp đồng:</td>
                            <td> @Html.DisplayFor(model => model.SoHD)</td>
                        </tr>
                        <tr>
                            <td>Nhà thầu:</td>
                            <td> @Html.DisplayFor(model => model.TenNhaThau)</td>
                        </tr>
                        <tr>
                            <td> Chỉ huy trưởng:</td>
                            <td>
                                @Html.DisplayFor(model => model.ChiHuyTruong)
                            </td>
                        </tr>



                        <tr>
                            <td>
                                Ngày dự kiến học
                            </td>
                            <td>
                                @Html.DisplayFor(model => model.NgayDuKienHoc)
                            </td>
                        </tr>
                    </table>
                </div>
                @*@{Html.RenderAction("ListAccount", "Employees", new { id = Model.MaNV });}*@
            </fieldset>

        </div>
    </div>
</div>
