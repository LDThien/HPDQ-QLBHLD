@using QLNHATHAU.Models
@model List<RegistrationlistValidation>
@if (Model.Count > 0)
{
    <h3 style="margin-bottom: 20px;">Thông tin khách đoàn</h3>
    <div class="table-responsive">
        <table class="table table-bordered table-striped table-hover" cellspacing="0">
            <thead class="thead-light">
                <tr>

                    <th class="text-center"> Họ Và Tên </th>
                    <th width="200px" class="text-center"> CMNN </th>

                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="text-center">@item.HoTen</td>
                            <td class="text-center">@item.CMND</td>
                        </tr>
                    }
                }
            </tbody>
        </table>

    </div>
}



